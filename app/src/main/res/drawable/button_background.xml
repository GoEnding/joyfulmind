<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#F4C542"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#F8E8C0"/>
            <corners android:radius="8dp"/>
            <stroke
                android:width="1dp"
                android:color="#F4C542"/>
        </shape>
    </item>
</selector>
